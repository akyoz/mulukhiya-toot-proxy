<%# encoding: UTF-8 -%>
SELECT
  value AS tag
FROM
  (
    SELECT
      jsonb_array_elements(fields)->>'value' AS value
    FROM users
    WHERE (local = true)
      AND (fields IS NOT null)
      AND (fields <> '[]'::jsonb)
      AND (fields <> '{}'::jsonb)
  ) AS t
WHERE (value LIKE '#%')
GROUP BY
  value
